				"videoCompression": "auto",
				"useTtlByStatus": true,
				"ttl": {
					"ok": 300,
					"redirects": 300,
					"clientError": 60,
					"serverError": 10
				}
			},
			"imageFiles": {
				"regex": "\\.(jpe?g|png|gif|webp|avif)$",
				"cacheability": true,
				"videoCompression": "auto",
				"useTtlByStatus": true,
