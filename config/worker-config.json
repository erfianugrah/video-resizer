					"ok": 300,
					"redirects": 300,
					"clientError": 60,
					"serverError": 10
				}
			},
			"highTraffic": {
				"regex": ".*\\/popular\\/.*\\.mp4",
				"cacheability": true,
				"videoCompression": "auto",
				"useTtlByStatus": true,
				"ttl": {
					"ok": 604800,
					"redirects": 300,
					"clientError": 60,
					"serverError": 10
