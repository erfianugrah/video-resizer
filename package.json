{
	"name": "video-resizer",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"deploy": "wrangler deploy",
		"deploy:prod": "wrangler deploy --env production",
		"deploy:staging": "wrangler deploy --env staging",
		"deploy:full": "npm run typecheck && npm run lint && wrangler deploy",
		"deploy:full:prod": "npm run typecheck && npm run lint && wrangler deploy --env production",
		"deploy:full:staging": "npm run typecheck && npm run lint && wrangler deploy --env staging",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"dev:with-ui": "npm run build:debug-ui:advanced && wrangler dev",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:integration": "vitest run test/integration",
		"test:services": "vitest run test/services",
		"test:utils": "vitest run test/utils",
		"test:domain": "vitest run test/domain",
		"test:edge-cases": "vitest run test/edge-cases",
		"typecheck": "tsc --noEmit",
		"typecheck:debug-ui": "cd debug-ui && npm run check",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"lint:src": "eslint src/",
		"lint:debug-ui": "eslint debug-ui/src/",
		"format": "prettier --write .",
		"cf-typegen": "wrangler types",
		"build": "tsc --noEmit",
		"build:debug-ui": "cd debug-ui && npm run build-debug-ui",
		"build:debug-ui:advanced": "cd debug-ui && npm run build-and-copy",
		"clean:debug-ui": "cd debug-ui && npm run clean",
		"clean:all": "npm run clean:debug-ui && rm -rf public/assets",
		"copy:debug-ui": "mkdir -p public && cd debug-ui && npm run copy-to-worker",
		"dev:debug-ui": "cd debug-ui && npm run dev"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.8.0",
		"@cloudflare/workers-types": "^4.20250312.0",
		"@types/node": "^20.11.24",
		"@types/uuid": "^10.0.0",
		"@typescript-eslint/eslint-plugin": "^7.4.0",
		"@typescript-eslint/parser": "^7.4.0",
		"@vitest/coverage-v8": "^3.0.8",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"prettier": "^3.2.5",
		"typescript": "^5.5.2",
		"typescript-eslint": "^8.26.1",
		"vitest": "^3.0.8",
		"wrangler": "^4.4.0"
	},
	"dependencies": {
		"pino": "^9.6.0",
		"pino-pretty": "^13.0.0",
		"uuid": "^11.1.0",
		"zod": "^3.24.2"
	}
}
